import java.util.*;
public class main
{
public static void main(string[] agrs)
    {
        scanner sc=new scanner(system.in);
        system.out.println("enter a number");
        int n=sc.nextint();
        int i,count=0;
        for(i=1;i<=n;i++)
        {
            if(n%i==0)
            count++; 
        }
        system.out.print("number of factors of "+n+" is "+count);
    }
}
